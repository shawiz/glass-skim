{
  "manifest_version": 2,
  "name": "Google Glass PlayGround Hack",
  "description": "This extension displays a \"dislike\" button on Facebook timeline.",
  "version": "1.0",


  "content_scripts": [ {
    "js": [ "./js/jquery-1.7.2.min.js","hack.js"],
    "matches": [ "https://developers.google.com/*", "http://developers.google.com/*", "https://apis.google.com/*"],
    "run_at": "document_end",
    "css": ["./stylesheets/tipsy.css"]
  } ],

  
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Google PlayGround Hacker",
    "default_popup": "popup.html"
  },

  "web_accessible_resources": [
      "hack.js",
      "js/jquery-1.7.2.min.js",
      "*://*.google.com/*"
  ],

  "permissions": [
      "http://*/"
  ]
}